local function script()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TomScripts/TomHub/refs/heads/main/all-in-one"))()
end
local function farm()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TomScripts/TomHub/refs/heads/main/loading"))()
end

task.spawn(script)
task.spawn(farm)
