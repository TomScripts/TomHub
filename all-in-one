local function script()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TomScripts/TomHub/refs/heads/main/mm2new"))()
end
local function farm()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TomScripts/TomHub/refs/heads/main/mm2autofarm"))()
end

task.spawn(script)
task.spawn(farm)
