local function script()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TomScripts/TomHub/refs/heads/main/mm2script"))()
end
local function farm()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TomScripts/TomHub/refs/heads/main/loading!"))()
end

task.spawn(script)
task.spawn(farm)
